@model GroceryList.DAL.Models.Product

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Quantity</th>
            <th>Units</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.Name</td>
            <td>@Model.Quantity</td>
            <td>@Model.units</td>
        </tr>
    </tbody>
</table>

@using (Html.BeginForm("ListOfProduct", "Product"))
{
    <input type="submit" value="Come back" />
}

